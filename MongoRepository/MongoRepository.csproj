<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.0.1.2</AssemblyVersion>
    <PackageId>Joha.MongoDbRepository</PackageId>
    <Authors>Jahongir Murtozayev</Authors>
    <Company>none</Company>
    <Product>Joha.MongoDbRepository</Product>
    <FileVersion>1.0.1.2</FileVersion>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/mirjahongirbek/MongoRepository</RepositoryUrl>
    <RepositoryType>C#</RepositoryType>
    <PackageTags>mongodb</PackageTags>
    <PackageReleaseNotes />
    <Version>1.1.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MongoDB.Driver" Version="2.7.0" />
    <PackageReference Include="StackExchange.Redis" Version="2.0.519" />
  </ItemGroup>

</Project>
